{
	"mcpServers": {
		"context7": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@upstash/context7-mcp"
			],
			"env": {}
		},
		"filesystem": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@modelcontextprotocol/server-filesystem",
				"."
			],
			"env": {}
		},
		"chrome-devtools": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"chrome-devtools-mcp@latest",
				"--headless=true",
				"--isolated=true"
			],
			"env": {}
		},
		"prisma-local": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"prisma",
				"mcp"
			],
			"env": {}
		},
		"prisma-remote": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"mcp-remote",
				"https://mcp.prisma.io/mcp"
			],
			"env": {}
		},
		"shadcn-ui": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@heilgar/shadcn-ui-mcp-server"
			],
			"env": {}
		},
		"ref-tools": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"ref-tools-mcp@latest"
			],
			"env": {}
		},
		"semgrep": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"semgrep-mcp"
			],
			"env": {}
		},
		"pieces": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@pieces.app/mcp-server"
			],
			"env": {}
		},
		"docker-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@quantgeekdev/docker-mcp"
			],
			"env": {}
		},
		"taskmaster": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"taskmaster-mcp"
			],
			"env": {}
		},
		"task-master-ai": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"task-master-ai"
			],
			"env": {
				"ANTHROPIC_API_KEY": "YOUR_ANTHROPIC_API_KEY_HERE",
				"PERPLEXITY_API_KEY": "YOUR_PERPLEXITY_API_KEY_HERE",
				"OPENAI_API_KEY": "YOUR_OPENAI_KEY_HERE",
				"GOOGLE_API_KEY": "YOUR_GOOGLE_KEY_HERE",
				"XAI_API_KEY": "YOUR_XAI_KEY_HERE",
				"OPENROUTER_API_KEY": "YOUR_OPENROUTER_KEY_HERE",
				"MISTRAL_API_KEY": "YOUR_MISTRAL_KEY_HERE",
				"AZURE_OPENAI_API_KEY": "YOUR_AZURE_KEY_HERE",
				"OLLAMA_API_KEY": "YOUR_OLLAMA_API_KEY_HERE"
			}
		}
	}
}
